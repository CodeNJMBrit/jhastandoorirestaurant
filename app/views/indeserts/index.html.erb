<p id="notice"><%= notice %></p>

<h1>Indeserts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Information</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @indeserts.each do |indesert| %>
      <tr>
        <td><%= indesert.name %></td>
        <td><%= indesert.information %></td>
        <td><%= indesert.price %></td>
        <td><%= link_to 'Show', indesert %></td>
        <td><%= link_to 'Edit', edit_indesert_path(indesert) %></td>
        <td><%= link_to 'Destroy', indesert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Indesert', new_indesert_path %>
